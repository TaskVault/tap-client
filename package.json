{
  "name": "tonads",
  "description": "NextJS WEB UI for Ton Ads platform",
  "keywords": [
    "Ton Ads",
    "blockchain",
    "e-commerce"
  ],
  "version": "0.1.0",
  "private": true,
  "repository": {
    "url": "https://github.com/TonAds/tonads",
    "type": "git"
  },
